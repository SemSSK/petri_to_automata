
MODULE main
    VAR
        s : {102,111,120};
		p0 : 0..1;
		p1 : 0..2;
		p2 : 0..2;
    ASSIGN
        init(s) := 102;

        next(s) := case
		s=102 : {111};
		s=111 : {120};
		s=120 : {102};
        esac;
        
        next(p0) := case
 		s=102 : {1};
		s=111 : {1};
		s=120 : {1}; 
		esac;
		next(p1) := case
 		s=102 : {1};
		s=111 : {2};
		s=120 : {0}; 
		esac;
		next(p2) := case
 		s=102 : {1};
		s=111 : {0};
		s=120 : {2}; 
		esac;

