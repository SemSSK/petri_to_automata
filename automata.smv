
MODULE main
    VAR
        s : {s120,s111,s102};
		p0 : 0..1;
		p1 : 0..2;
		p2 : 0..2;
    ASSIGN
        init(s) := s120;

        next(s) := case
		s=s120 : {s102};
		s=s111 : {s120};
		s=s102 : {s111};
        esac;
        
        next(p0) := case
 		s=s120 : {1};
		s=s111 : {1};
		s=s102 : {1}; 
		esac;
		next(p1) := case
 		s=s120 : {0};
		s=s111 : {2};
		s=s102 : {1}; 
		esac;
		next(p2) := case
 		s=s120 : {2};
		s=s111 : {0};
		s=s102 : {1}; 
		esac;

