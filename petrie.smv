
MODULE main
    VAR
        s : {s_1_0_2,s_0_1_2};
		A : 0..1;
		B : 0..1;
		C : 0..2;
    ASSIGN
        init(s) := s_1_0_2;

        next(s) := case
		s=s_1_0_2 : {s_0_1_2};
		s=s_0_1_2 : {s};
        esac;
        
        next(A) := case
 		s=s_1_0_2 : {0};
		s=s_0_1_2 : {A}; 
		esac;
		next(B) := case
 		s=s_1_0_2 : {1};
		s=s_0_1_2 : {B}; 
		esac;
		next(C) := case
 		s=s_1_0_2 : {2};
		s=s_0_1_2 : {C}; 
		esac;

